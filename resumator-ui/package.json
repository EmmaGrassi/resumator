{
  "name": "resumator",
  "version": "1.0.0",
  "scripts": {
    "start": "gulp develop",
    "test": "gulp runTests",
    "docker:start": "cd ..; docker-machine start default; eval $(docker-machine env default);",
    "maven:start": "cd ..; mvn clean install; RESUMATOR_CONFIG=~/.resumator docker-compose -f ./docker/docker-compose-dev.yml up;"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "rules": {
      "max-len": 0,
      "consistent-return": 0,
      "new-cap": 0,
      "no-param-reassign": 0,
      "no-shadow": 0,
      "no-unused-vars": 0,
      "react/jsx-no-bind": 0,
      "react/prop-types": 0,
      "no-alert": 0,
      "no-console": 0,
      "no-case-declarations": 0,
      "react/prefer-stateless-function": 0
    },
    "env": {
      "mocha": true
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "body-parser": "^1.14.2",
    "chai": "^3.4.0",
    "cookies-js": "^1.2.2",
    "domready": "^1.0.8",
    "express": "^4.13.3",
    "fixed-data-table": "^0.6.0",
    "history": "^1.17.0",
    "immutable": "^3.7.6",
    "lodash": "^3.10.1",
    "loglevel": "^1.4.0",
    "moment": "^2.10.6",
    "parameterize": "0.0.7",
    "phone": "^1.0.5",
    "qwest": "^2.2.6",
    "react": "^0.14.2",
    "react-bootstrap": "^0.28.1",
    "react-dom": "^0.14.2",
    "react-loader": "^2.0.0",
    "react-quill": "^0.4.1",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.3",
    "react-router-bootstrap": "^0.19.3",
    "react-tagged-input": "0.0.18",
    "react-tags": "^0.2.2",
    "react-tagsinput": "^3.0.3",
    "redux": "^3.0.4",
    "redux-devtools": "^2.1.5",
    "redux-logger": "^2.3.1",
    "redux-promise": "^0.5.0",
    "redux-simple-router": "^1.0.2",
    "redux-thunk": "^1.0.3",
    "request": "^2.65.0",
    "request-promise": "^1.0.2",
    "seamless-immutable": "^5.0.1",
    "superagent": "^1.7.0",
    "tcomb-form": "^0.7.10",
