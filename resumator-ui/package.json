  "devDependencies": {
    "babel": "^6.3.13",
    "babel-core": "^6.2.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "browser-sync": "^2.11.0",
    "browserify": "^12.0.1",
    "chai-enzyme": "^0.4.1",
    "enzyme": "^2.1.0",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^6.0.2",
    "eslint-plugin-babel": "^3.1.0",
    "eslint-plugin-react": "^4.1.0",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.1",
    "gulp-browserify": "^0.5.1",
    "gulp-changed": "^1.3.0",
    "gulp-commonjs-tasks": "^1.2.0",
    "gulp-cssnano": "^2.0.0",
